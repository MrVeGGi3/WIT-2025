class_name Charizard
extends Fogo

func _ataque_asa (dano, pokémon):
	print("O pokémon %s sofreu %d dano do Ataque de Asa!" % [pokémon, dano])

func _furacão(dano, pokémon):
	print("O pokémon %s sofreu %d dano do Ataque Furacão" % [pokémon, dano])

func _garra_dragão(dano, pokémon):
	print("O pokémon %s sofreu %d dano do Ataque Garra do Dragão" % [pokémon, dano])
